{
    "name": "mikailfaruqali/guardian",
    "description": "A powerful Laravel security package with file upload protection, dual 2FA authentication (Email for masters, Google Authenticator for users), session management, and beautiful UI",
    "keywords": [
        "laravel",
        "2fa",
        "two-factor",
        "authentication",
        "security",
        "file-upload",
        "google-authenticator",
        "guardian",
        "snawbar"
    ],
    "homepage": "https://github.com/mikailfaruqali/guardian",
    "license": "MIT",
    "authors": [
        {
            "name": "Snawbar",
            "email": "alanfaruq85@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0",
        "pragmarx/google2fa-laravel": "^1.4|^2.0",
        "intervention/image": "^2.5|^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "driftingly/rector-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Snawbar\\Guardian\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Snawbar\\Guardian\\GuardianServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}