{
    "name": "mikailfaruqali/guardian",
    "description": "Modern Laravel two-factor authentication package with dual authentication system: email-based 2FA for master users and Google Authenticator for regular users. Features beautiful Tailwind CSS UI, multilingual support (English/Kurdish/Arabic) with RTL layout, configurable logo/fonts, rate limiting, and seamless Laravel integration.",
    "keywords": [
        "laravel",
        "2fa",
        "two-factor",
        "authentication",
        "security",
        "file-upload",
        "google-authenticator",
        "guardian",
        "snawbar"
    ],
    "homepage": "https://github.com/mikailfaruqali/guardian",
    "license": "MIT",
    "authors": [
        {
            "name": "Snawbar",
            "email": "alanfaruq85@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": ">=5.0",
        "pragmarx/google2fa": "^8.0",
        "simplesoftwareio/simple-qrcode": "^4.2"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.22 || ^9.0",
        "driftingly/rector-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Snawbar\\Guardian\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Snawbar\\Guardian\\GuardianServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}